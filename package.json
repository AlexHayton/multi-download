{
	"name": "multi-download",
	"version": "2.0.1",
	"description": "Download multiple files at once in the browser",
	"license": "MIT",
	"repository": "sindresorhus/multi-download",
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "sindresorhus.com"
	},
	"scripts": {
		"build": "a=$npm_package_name; browserify --require=.:$a --standalone=$a --outfile=browser.js index.js"
	},
	"files": [
		"index.js"
	],
	"keywords": [
		"browser",
		"download",
		"multiple",
		"parallel",
		"files"
	],
	"devDependencies": {
		"browserify": "^16.2.3"
	}
}
